/**app.wxss**/
@import "./assets/iconfont/iconfont.wxss";

.container{
  padding: 15rpx 8rpx;
  height: 100vh;
  display: flex;
  flex-direction: column;
  background-color: #f5fafe;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #333;
} 
.copyright{
  align-self: center; 
  flex: 1;
  display: flex;
  align-items: flex-end;
  padding-bottom: 20rpx;
  font-size: 14px;
  color:#999;
}
.view-hover {
  background-color: #f3f3f3!important;
}
